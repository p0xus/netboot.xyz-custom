#!ipxe
###
### netboot.xyz-custom menu
###

:custom
clear custom_choice
menu p0xnet selection menu
item --gap Thinstation tests
item load_thinstation ${space} Load Thinstation
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:load_thinstation
kernel https://p0xnet.net/thinstation/vmlinuz splash=off console=tty1 LM=3
initrd https://p0xnet.net/thinstation/initrd
boot || goto custom_exit

:custom_exit
chain utils.ipxe
exit
